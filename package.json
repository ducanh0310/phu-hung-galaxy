{
  "name": "demoapp",
  "private": true,
  "version": "1.0.0",
  "description": "Gourmet Grove Monorepo",
  "type": "module",
  "scripts": {
    "dev": "vite frontend",
    "dev:all": "concurrently \"npm:dev:backend\" \"npm:dev:frontend\"",
    "dev:frontend": "vite frontend",
    "dev:backend": "nodemon --exec tsx backend/src/index.ts",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "vite build frontend",
    "build:backend": "tsc -p backend/tsconfig.json",
    "start": "node backend/dist/src/index.js",
    "preview": "vite preview frontend",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed",
    "postinstall": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.3",
    "prisma": "^5.15.0",
    "tsx": "^4.20.3",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  },
  "prisma": {
    "schema": "backend/prisma/schema.prisma",
    "seed": "tsx backend/prisma/seed.ts"
  }
}
